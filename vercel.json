{
  "builds": [
    {
      "src": "apps/api/package.json",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/signup",
      "destination": "/apps/auth/pages/api/signup"
    },
    {
      "source": "/api/signin",
      "destination": "/apps/auth/pages/api/signin"
    },
    {
      "source": "/api/signout",
      "destination": "/apps/auth/pages/api/signout"
    },
    {
      "source": "/(.*)",
      "destination": "/api/$1"
    }
  ]
}
