{
    "version": 2,
    "builds": [
      {
        "src": "pages/api/**/*.ts",
        "use": "@vercel/node"
      },
      {
        "src": "pages/api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/pages/api/$1"
      }
    ]
  }
  